    // Contact Us Section
    section#contact.contact-us-section.section-padding
      .container
        // Contact Us Section Title
        h2.section-title.contact-title.text-center Contact Us
        p.sub-title.contact-subtitle.text-center
          | If you have any questions, suggestions, or comments please reach out.
          br
          | Our customer service phone number is: xxx-xxxx-xxxx
        .row
          .col-md-10.col-md-offset-1.col-lg-10.col-lg-offset-1.col-sm-10.col-sm-offset-1.col-xs-12
            .contact-form-section.mt-50
              form#contactForm(method='POST', action='', name='contact-form', role='form')
                .row
                  .col-sm-5.col-md-4
                    // Name Field
                    .form-group.contact-form-icon
                      label.sr-only Name
                      i.fa.fa-user
                      input#name.form-control(type='text', placeholder='Name', name='name', required='')
                    // Email Field
                    .form-group.contact-form-icon
                      label.sr-only(for='email') Email
                      i.fa.fa-envelope
                      input#email.form-control(type='email', placeholder='Email', name='email', required='')
                    // Subject Field
                    .form-group.contact-form-icon
                      label.sr-only(for='subject') Subject
                      i.fa.fa-pencil
                      input#subject.form-control(type='text', placeholder='Subject', name='subject', required='')
                  .col-sm-7.col-md-8
                    .form-group.contact-form-icon
                      label.sr-only(for='message') Message
                      i.fa.fa-keyboard-o
                      textarea#message.form-control(placeholder='Message', rows='7', name='message', required='')
                  .col-sm-5.col-md-4.text-center.contact-button-padding
                    button.btn-primary.btn-contact.btn-block(name='submit', type='submit')
                      i.fa.fa-paper-plane
                      | Send
    // End Of Contact Us Section
    // Google Map Section
    section.goolge-map-section
      // Toggle Button Area
      .toggle-map-button
        button#mapToggle.gmap-btn.btn-block
          | Locate on google map
          i.fa.fa-chevron-down
      // Google Map
